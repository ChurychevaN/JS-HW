n      console.log(event.target.value);\n      break;\n    case \"end_week\":\n        dataChangeHandler(startWeekElement, \"max\", event.target.value);\n      console.log(event.target.value);\n      break;\n    case \"end_month\":\n        dataChangeHandler(startMonthElement, \"max\", event.target.value);\n      console.log(event.target.value);\n      break;\n  }\n});\n\n// if (event.target.id === 'start_date' || event.target.id === 'start_week' || event.target.id === 'start_month') {\n//     endDateElement.setAttribute(\"min\", event.target.value); //Встановлює значення атрибута для зазначеного елемента.\n//     console.log(event.target.value);\n// }\n// if (event.target.id === 'end_date') {\n// startDateElement.setAttribute(\"max\", event.target.value);\n// console.log(event.target.value);\n// } else {\n//     console.log('day');\n// }\n\n// 2. Юзер може обрати пресети проміжків часу:\n//  - 'тиждень'\n// const changeWeekContainer = document.querySelector('.change-week');\n\n// const startWeekElement = changeWeekContainer.querySelector('#start_week');\n// const endWeekElement = changeWeekContainer.querySelector('#end_week');\n\n// changeWeekContainer.addEventListener(\"change\", (event) => {\n\n//     console.log(event.target.id);\n\n//     if (event.target.id === 'start_week') {\n//         endWeekElement.setAttribute(\"min\", event.target.value); //Встановлює значення атрибута для зазначеного елемента.\n//         console.log(event.target.value);\n//     }\n//     if (event.target.id === 'end_week') {\n//         // startWeekElement.setAttribute(\"max\", event.target.value);\n\n//         console.log(event.target.value);\n//     } else {\n//         console.log('week');\n//     }\n\n// });\n\n// - 'місяць'\n// const changeMonthContainer = document.querySelector('.change-month');\n\n// const startMonthElement = changeMonthContainer.querySelector('#start_month');\n// const endMonthElement = changeMonthContainer.querySelector('#end_month');\n\n// changeMonthContai